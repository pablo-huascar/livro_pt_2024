@ARTICLE{Pascoal2008-gx,
  title     = "{O} orientador educacional no Brasil",
  author    = "Pascoal, Miriam and Honorato, Eliane Costa and Albuquerque,
               Fabiana Aparecida de",
  abstract  = "Historicamente, a orienta{\c c}{\~a}o educacional tem sido uma
               das fun{\c c}{\~o}es exercidas pelo profissional da educa{\c
               c}{\~a}o denominado pedagogo. Nos diferentes estados brasileiros
               e nas diferentes redes escolares, este profissional, se
               existente, recebe denomina{\c c}{\~o}es variadas e exerce
               atividades tamb{\'e}m variadas. Tal diversidade descaracteriza a
               real dimens{\~a}o de seu fazer profissional e estabelece um
               conflito entre os diversos pap{\'e}is desempenhados pelos
               diferentes profissionais da educa{\c c}{\~a}o. Neste sentido,
               este artigo apresenta um mapeamento da exist{\^e}ncia desse
               profissional na rede p{\'u}blica estadual brasileira, al{\'e}m
               de uma discuss{\~a}o cr{\'\i}tica sobre a especificidade do
               trabalho do orientador educacional, apontando para a necessidade
               de sua presen{\c c}a em todas as escolas da rede escolar
               brasileira.",
  journal   = "Educação em Revista",
  publisher = "FapUNIFESP (SciELO)",
  number    =  47,
  pages     = "101--120",
  month     =  jun,
  year      =  2008,
  url       = {http://dx.doi.org/10.1590/S0102-46982008000100006}
}


